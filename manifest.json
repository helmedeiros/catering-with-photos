{
  "manifest_version": 3,
  "name": "Catering with Photos",
  "description": "Enhances Z-Catering menu by displaying food images next to each menu item",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://bestellung.z-catering.de/*"],
      "js": ["content.js"]
    }
  ]
}
